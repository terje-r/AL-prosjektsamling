function scrollToActiveCategory(n){if(n&&n.length){const t=n.find(".cw-categories__item--active");if(t&&t.length){const i=t.position().left-n.width()/2+t.width()/2;_didCategoriesScroll=!0;n.animate({scrollLeft:i},0)}}}let _didCategoriesScroll=!1;scrollToActiveCategory($(".cw-categories--inner > .cw-categories__list"));scrollToActiveCategory($(".cw-categories__body > .cw-categories__list"));$(document).ready(function(){_didCategoriesScroll||scrollToActiveCategory()});$(document).ready(function(){const n=window.webshopPaginationConfig;if(n){const h=n.allergens||[],i=n.category;let u=n.offset;const f=n.numberOfProducts,c=n.total;let o=2,e=n.remaining;const l=n.offersOnly,a=n.elementToInject,s=$(".cw-pagination__loader"),r=$("#cw-loadmore-btn"),v=n.query||"",y=n.fallbackBestSales,p=n.addHouseHoldId,w=n.sort||"",b=n.campaignId,k=n.oftenbought;let t=n.blockId;e>f&&r.removeClass("cw-visuallyhidden");$("#cw-loadmore-btn").click(function(){s.removeClass("hidden");r.addClass("cw-visuallyhidden");const n=$(window).scrollTop();t||(t="ikke-noe-id");$.ajax({url:"/WSPager?categorySlug1="+i[0]+"&categorySlug2="+i[1]+"&categorySlug3="+i[2]+"&categorySlug4="+i[3]+"&offersOnly="+l+"&page="+o+"&pageSize="+f+"&query="+v+"&sort="+w+"&allergens="+h+"&fallbackBestSales="+y+"&addHouseHoldId="+p+"&id="+b+"&oftenBought="+k+"&blockId="+t}).done(function(i){s.addClass("hidden");console.log("block id: "+t);let h=i;h=$(h).html();$(a).append(h);$(window).scrollTop(n);window.updateHabitat();o++;u=u+f;e=c-u;e<=0?r.addClass("cw-visuallyhidden"):r.removeClass("cw-visuallyhidden")}).fail(function(n){console.log("Error: "+n.status)});console.log("block id helt pÃ¥ slutten: "+t)})}});$(document).ready(function(){if(typeof Bloodhound!="undefined"){const n=$("#cw-product-search__input"),t=new Bloodhound({datumTokenizer:function(n){return Bloodhound.tokenizers.whitespace(n.name)},queryTokenizer:Bloodhound.tokenizers.whitespace,remote:{wildcard:"%QUERY",url:"/products/autosuggestall/?query=%QUERY",transform:function(t){const i=$.map(t,function(n){return{category:n.category,group:n.group,url:n.slugifiedurl}}),r=n.typeahead("val"),u={group:!1,url:"/nettbutikk-sok?query="+encodeURI(r),allElement:!0,searchTerm:r};return i.push(u),i}}});t.initialize();n.typeahead({highlight:!0,minLength:2},{name:"productSearch",source:t,limit:25,displayKey:"name",templates:{suggestion:function(n){return n.group?"<span class='cw-product-search__resultlist-item'><a class='' href='"+n.url+"'><span class='' >"+n.group+"<\/span> i "+n.category+"<\/a><\/span>":n.allElement?"<span class='cw-product-search__resultlist-item'><a class='' href='"+n.url+"'>Se alle treff p&aring; "+n.searchTerm+"<\/span>":"<span class='cw-product-search__resultlist-item'><a class='' href='"+n.url+"'>"+n.category+"<\/a><\/span>"}}});n.bind("typeahead:select",function(n,t){n.preventDefault();window.location.href=t.url})}})